RewriteEngine on
edirect 301 / https://ktms.com.hk/
Redirect 301 /index.html https://www.ktms.com.hk/
Redirect 301 /index-zh.html https://www.ktms.com.hk/
RewriteBase /
RewriteRule (.*) https://ktms.com.hk/$1 [R=301,L]
Options +FollowSymLinks
RewriteEngine On
RewriteRule ^(.*) https://www.ktms.com.hk%{REQUEST_URI} [R=302,NC]
